<?xml version="1.0" encoding="UTF-8"?>
<beans     xmlns="http://www.springframework.org/schema/beans"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
           default-autowire="constructor">
  <bean class="jetbrains.buildServer.dotTrace.server.DotTraceEditExtension"/>

  <bean class="jetbrains.buildServer.dotTrace.server.ValueAggregatorFirst" id="valueAggregatorFirst"/>
  <bean class="jetbrains.buildServer.dotTrace.server.ValueAggregatorLast" id="valueAggregatorLast"/>
  <bean class="jetbrains.buildServer.dotTrace.server.ValueAggregatorAverage" id="valueAggregatorAverage"/>

  <bean class="jetbrains.buildServer.dotTrace.server.ValueAggregatorFactoryImpl" scope="prototype">
    <constructor-arg ref="valueAggregatorFirst"/>
    <constructor-arg ref="valueAggregatorLast"/>
    <constructor-arg ref="valueAggregatorAverage"/>
  </bean>
</beans>
